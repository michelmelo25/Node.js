    "eslint": "^8.57.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "sqlite3": "^5.1.7"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
